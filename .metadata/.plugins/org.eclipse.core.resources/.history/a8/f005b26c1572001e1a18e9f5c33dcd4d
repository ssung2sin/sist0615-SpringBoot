server:
  port: 9005
spring:
  mvc:
    view:
    #jsp setting
      suffix: .jsp
      prefix: /WEB-INF/
  datasource:
    driver-class-name: com.mysql.cj.jdbc.Driver
    url: jdbc:mysql://localhost:3306/coffee?serverTimezone=Asia/Seoul
    username: ssung2sin
    password: a1234
#스키마생성 create:기존테이블을 삭제후 생성,update:변경된부분만 반영
  jpa:
    hibernate:
      ddl-auto: auto
#ddl생성시 데이터베이스의 고유기능 사용할지
spring.jpa.generate-ddl=true
#api호출시 실행되는 sql문을 콘솔에 보여줄지
spring.jpa.show-sql=true
#사용할 데이터베이스 종류
spring.jpa.database=mysql
#mysql상세지정
#자동 새로고침
    #mysql상세지정
    database-platform: atform org.hibernate.dialect.MySQL5InnoDBDialect
  devtools:
    livereload:
      enabled: true
