server.port=9002
#mysql setting
spring.datasource.driver-class-name=
spring.datasource.url=jdbc:mysql://localhost:3306/coffee?serverTimezone=Asia/Seoul
spring.datasource.username=ssung2sin
spring.datasource.password=a1234
#JPA setting
#스키마생성 create:기존테이블을 삭제후 생성,update:변경된부분만 반영
spring.jpa.hibernate.ddl-auto=udate
#ddl생성시 데이터베이스의 고유기능 사용할지
spring.jpa.generate-ddl=true
#api호출시 실행되는 sql문을 콘솔에 보여줄지
spring.jpa.show-sql=true
#사용할 데이터베이스 종류
spring.jpa.database=mysql