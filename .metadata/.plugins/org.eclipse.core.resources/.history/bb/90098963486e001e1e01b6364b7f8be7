package boot.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class FormController {
	
	@GetMapping({"/","/sist/home"})
	public String home() {
		
		return "home";
	}
	
	@GetMapping("/fomr1")
	public String form1() {
		
		return "form/form1";
	}
	@GetMapping("/fomr2")
	public String form2() {
		
		return "form/form2";
	}
	@GetMapping("/fomr3")
	public String form3() {
		
		return "form/form3";
	}
}
